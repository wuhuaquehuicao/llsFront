<script src="/javascript/bootstrap-table.js"></script>
<script>
  (function ($) {
    'use strict';

    var sprintf = function (str) {
      var args = arguments,
        flag = true,
        i = 1;

      str = str.replace(/%s/g, function () {
        var arg = args[i++];

        if (typeof arg === 'undefined') {
          flag = false;
          return '';
        }
        return arg;
      });
      return flag ? str : '';
    };

    $.fn.bootstrapTable.locales = {
      formatLoadingMessage: function () {
        return '<%= __('loadingMessage') %>';
      },
      formatRecordsPerPage: function (pageNumber) {
        return sprintf('<%= __('recordsPerPage') %>', pageNumber);
      },
      formatShowingRows: function (pageFrom, pageTo, totalRows) {
        return sprintf('<%= __('showingRows') %>', pageFrom, pageTo, totalRows);
      },
      formatSearch: function () {
        return '<%= __('search') %>';
      },
      formatNoMatches: function () {
        return '<%= __('noMatches') %>';
      },
      formatPaginationSwitch: function () {
        return '<%= __('paginationSwitch') %>';
      },
      formatRefresh: function () {
        return '<%= __('refresh') %>';
      },
      formatToggle: function () {
        return '<%= __('toggle') %>';
      },
      formatColumns: function () {
        return '<%= __('columns') %>';
      },
    };

    $.extend($.fn.bootstrapTable.defaults, $.fn.bootstrapTable.locales);
  })(jQuery);
</script>
